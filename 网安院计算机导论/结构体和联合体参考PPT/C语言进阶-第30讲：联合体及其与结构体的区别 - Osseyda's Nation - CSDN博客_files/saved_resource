!function(){"use strict";$.Toast=function(t,s,o,e){var a=null,i=$.extend(!0,{},{appendTo:"body",stack:!1,position_class:"toast-bottom-right",fullscreen:!1,width:250,spacing:20,timeout:4e3,has_close_btn:!0,has_icon:!0,sticky:!1,border_radius:6,has_progress:!1,rtl:!1},e),n=i.spacing,r={position:"body"==i.appendTo?"fixed":"absolute","min-width":i.width,display:"none","border-radius":i.border_radius,"z-index":99999};if(a=$('<div class="toast-item-wrapper '+o+" "+i.position_class+'"></div>'),$('<p class="toast-title">'+t+"</p>").appendTo(a),$('<p class="toast-message">'+s+"</p>").appendTo(a),i.fullscreen&&a.addClass("fullscreen"),i.rtl&&a.addClass("rtl"),i.has_close_btn&&($('<span class="toast-close">&times;</span>').appendTo(a),i.rtl?r["padding-left"]=20:r["padding-right"]=20),i.has_icon&&($('<i class="toast-icon toast-icon-'+o+'"></i>').appendTo(a),i.rtl?r["padding-right"]=50:r["padding-left"]=50),i.has_progress&&0<i.timeout&&$('<div class="toast-progress"></div>').appendTo(a),i.sticky)switch(i.spacing=0,n=0,i.position_class){case"toast-top-left":case"toast-top-right":r.top=0,r.left=0;break;case"toast-top-center":r.top=0,r.left=r.right=0,r.width="100%";break;case"toast-bottom-left":r.bottom=0,r.left=0;break;case"toast-bottom-right":r.bottom=0,r.right=0;break;case"toast-bottom-center":r.bottom=0,r.left=r.right=0,r.width="100%"}function p(){window.csdn.userLogin.Toast.remove(a)}return i.stack&&(-1!==i.position_class.indexOf("toast-top")?$(i.appendTo).find(".toast-item-wrapper").each(function(){r.top=parseInt($(this).css("top"))+this.offsetHeight+n}):-1!==i.position_class.indexOf("toast-bottom")&&$(i.appendTo).find(".toast-item-wrapper").each(function(){r.bottom=parseInt($(this).css("bottom"))+this.offsetHeight+n})),a.css(r),a.appendTo(i.appendTo),a.fadeIn?a.fadeIn():$alert.css({display:"block",opacity:1}),0<i.timeout&&(setTimeout(p,i.timeout),i.has_progress&&$(".toast-progress",a).animate({width:"100%"},i.timeout)),$(".toast-close",a).click(p),a},window.csdn.userLogin=window.csdn.userLogin||{},window.csdn.userLogin.Toast=$.Toast,$.Toast.remove=function(t){t.fadeOut?t.fadeOut(function(){return t.remove()}):t.remove()},window.csdn.userLogin.Toast.remove=$.Toast.remove}();